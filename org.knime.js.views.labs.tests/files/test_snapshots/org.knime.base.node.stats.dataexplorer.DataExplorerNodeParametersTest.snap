            "default" : "10, 25, 50, 100"
          },
          "customCSS" : {
            "type" : "string",
            "default" : ""
          },
          "displayColumnHeaders" : {
            "type" : "boolean",
            "default" : true
          },
          "displayFullscreenButton" : {
            "type" : "boolean",
            "title" : "Display fullscreen button",
            "description" : "If you check this option, the interactive view will enable option to enlarge the interactive\nview.\n",
            "default" : true
          },
          "displayRowIds" : {
            "type" : "boolean",
            "title" : "Display Row ID in Data Preview",
