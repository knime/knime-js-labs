            "default" : true
          },
          "displayRowNumber" : {
            "type" : "integer",
            "format" : "int32",
            "title" : "Number of rows for data preview",
            "description" : "Determines the total number of rows displayed on the Data Preview tab of the interactive view.\n",
            "default" : 100
          },
